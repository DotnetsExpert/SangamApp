    <script>
        function gst() {
            //debugger;
            if (GST.value != '') {
                var CSGST = GST.value * 1 / 2;
                document.getElementById('CGST').value = CSGST;
                document.getElementById('SGST').value = CSGST;
            }
        }
        function Validate() {
            if (CategoryId.value == '') {
                alert('Please Select Category ');
                CategoryId.focus();

                return false;
            }
            if (Name.value == '') {
                alert('Enter Product Name');
                Name.focus();

                return false;
            }
            if (BatchNo.value == '') {
                alert('Enter Batch No');
                BatchNo.focus();

                return false;
            }
            if (HSNCode.value == '') {
                alert('Enter HSN Code');
                HSNCode.focus();

                return false;
            }
            if (PurchaseRate.value == '0') {
                alert('Enter Purchase Rate');
                PurchaseRate.focus();

                return false;
            }
            if (SaleRate.value == '0') {
                alert('Enter Sale Rate');
                SaleRate.focus();

                return false;
            }


        }


