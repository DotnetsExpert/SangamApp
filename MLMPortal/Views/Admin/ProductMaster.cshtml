        function DeleteSubCategoryDetails(SubCategoryId) {
            var x = confirm("Are you sure you want to delete?");
            //debugger;
            if (x) {
                var dataobject = {
                    SubCategoryId: SubCategoryId
                };
                $.ajax({
                    type: "POST",
                    url: "/Admin/DeleteSubCategoryDetails",
                    data: dataobject,
                    dataType: "json",
                    success: function (r) {
                        if (r.CategoryId == "1") {
                            alert(r.msg);
                            location.reload();
                        }
                        else {
                            alert(r.msg);
                        }
                    }
                });
            }
            else {
                return false;
            }
        }


        function ShowHideProduct(Action, Id) {

            var dataobject = {
                Id: Id,
                Action: Action
            };
            $.ajax({
                type: "POST",
                url: "/Admin/ShowHideProduct",
                data: dataobject,
                dataType: "json",
                success: function (r) {
                    if (r.Id == "1") {
                        alert(r.msg);
                        location.reload();
                    }
                    else {
                        alert(r.msg);
                    }
                }
            });

        }

        function BindSubcategory(_this) {
            //debugger;
            // alert(_this.value);

            if ($("#CategoryId option:selected").text() == "Package 555") {
                $("#APP").attr("disabled", "disabled");
                bindapp();
            }
            else {
                $("#APP").removeAttr("disabled", "disabled");
                bindapp();
            }


            var dataobject = {
                CategoryId: $("#CategoryId").val()

            };
            $.ajax({
                type: "POST",
                url: "/Admin/BindSubCategory",
                data: dataobject,
                dataType: "json",
                success: function (r) {

                    $('#SubCategoryId').html(MakeDropdown(r));


                }
            });

        }

