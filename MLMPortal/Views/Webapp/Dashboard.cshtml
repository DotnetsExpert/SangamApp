.blinkcall {
    position: fixed;
    z-index: 10;
    bottom: 5%;
    right: 0%;
    transform: translateX(-50%) translateY(-50%);
    box-sizing: content-box;
    display: block;
    width: 32px;
    height: 44px;

    border-radius: 50%;
    padding: 18px 20px 18px 28px;
}





.blinkcall:before {
  content: "";
  position: absolute;
  z-index: 0;
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%);
  display: block;
  width: 80px;
  height: 80px;
  background: #1ba710;
  border-radius: 50%;
  animation: pulse-border 1500ms ease-out infinite;
}

    .blinkcall span {
        display: block;
        position: relative;
        z-index: 3;
        width: 50px;
        height: 50px;
        background: url(../../App/img/home_slider-img.png);
        background-size: cover;
        background-position: center;
        margin-left: -12px;
        margin-top: -2px;
    }





.blinkcall:after {
  content: "";
  position: absolute;
  z-index: 1;
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%);
  display: block;
  width: 80px;
  height: 80px;
  background: #fff;
  border-radius: 50%;
  transition: all 200ms;
  box-shadow: 0px 0px 20px rgb(0 0 0 / 32%);
}




.blinkcall:hover:after {
 /* background-color: darken(#fa183d, 10%);*/
}
@@keyframes pulse-border {
  0% {
    transform: translateX(-50%) translateY(-50%) translateZ(0) scale(1);
    opacity: 1;
  }
  100% {
    transform: translateX(-50%) translateY(-50%) translateZ(0) scale(1.5);
    opacity: 0;
  }
}


