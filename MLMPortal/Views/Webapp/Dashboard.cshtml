    });


    function openmodal(){
        $.ajax({
            url: '../App/ModalOtpCon',
            type: 'post',
            dataType: 'json',
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                var s = data.Id;
                if (s != 0) {
                    $('#secAddres').hide();
                    $('#secVarification').show();

                }
                else {
                    $('#secAddres').show();
                    $('#secVarification').hide();
                }
            }
        })
    }




    function Show1() {
        $('#loading').fadeIn(1000);

        $.post('/App/Click_Mining', {}, function (Res) {

            Hide();
            if (Res != undefined && Res.action ==1) {

                window.location.href = "/App/Dashboard";
            }


        })



    }
    function Hide() {
        $('#loading').fadeOut(1000);
    }




    function SendOTP() {
        $("#showSpinner").show();
        var dataObj = { Address: $('#Address').val(), Name: $('#Name').val() }
        $.ajax({
            url: '../App/SendOTP',
            type: 'post',
            dataType: 'json',
            data: JSON.stringify(dataObj),
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                if (data.Msg != '0') {
                    alert(data.Msg);
                    $("#showSpinner").hide();
                    $('#secAddres').hide();
                    $('#secVarification').show();
                }
                else {
                    $("#showSpinner").hide();
                    alert('Server Error Occured');
                    location.reload();
                }

            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {

                $("#showSpinner").hide();
            }
        });

    }


    function DeleteAddress() {
        var conf = confirm('Are you sure! You want to delete address?');
        if (conf) {
            $("#showSpinner").show();
            $.ajax({
                url: 'home.aspx/DeleteAddress',
                type: 'post',
                dataType: 'json',
                data: JSON.stringify({ AddressId: $('#AddressId').val() }),
                contentType: 'application/json; charset=utf-8',
                success: function (data) {
                    if (data.d != '0') {
                        var val = eval(data.d);
                        $.each(val, function (k, v) {
                            if (v.ID == '1') {
                                alert(v.Msg);
                                $("#showSpinner").hide();
                                location.reload();
                            }
                            else {
                                $("#showSpinner").hide();
                                alert('Server error occured.');
                            }
                        });
                    }
                    else {
                        $("#showSpinner").hide();
                        alert('Id not foud.');
                    }
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {

                    $("#showSpinner").hide();
                }
            })
        }
    }



    function SaveAddress() {
        $("#showSpinner").show();
        var obj = { OTP: $('#OTP').val(), Password: $('#Password').val() };
        $.ajax({
            url: '../App/SaveAddress',
            type: 'post',
            dataType: 'json',
            data: JSON.stringify(obj),
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                var s = data.Id;
                if (s != '0') {
                    alert(data.Msg);
                    $("#showSpinner").hide();
                    //$('#addresModal').modal('hide');
                    location.reload();
                }
                else{
                    $("#showSpinner").hide();
                    alert(data.Msg);
                }

            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {

                $("#showSpinner").hide();
            }
        })
    }



    function checkmobileweb() {
        var isMobile = {
            Android: function () {
                return navigator.userAgent.match(/Android/i);
            },
            BlackBerry: function () {
                return navigator.userAgent.match(/BlackBerry/i);
            },
            iOS: function () {
                return navigator.userAgent.match(/iPhone|iPad|iPod/i);
            },
            Opera: function () {
                return navigator.userAgent.match(/Opera Mini/i);
            },
            Windows: function () {
                return navigator.userAgent.match(/IEMobile/i);
            },
            any: function () {
                return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
            }
        }; if (isMobile.any()) {
            //   alert($("#ContentPlaceHolder1_lblhdnref").val());
            // $("#reflnk").attr('href', $("#lblhdnref").val());
            window.location.href = "../App/Term";
            //alert("whatsapp://send");

        } else {
            // alert("https://web.whatsapp.com/send");
            window.location.href = "../App/Term";
        }
    }


</script>

